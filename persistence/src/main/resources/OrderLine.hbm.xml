<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
    <class name="app.domain.OrderLine" table="OrderLines">
        <composite-id>
            <key-property name="orderId" column="OrderId" access="field" />
            <key-property name="sku" column="Sku" access="field" />
            <key-property name="quantity" column="Quantity" access="field" />
        </composite-id>
    </class>
</hibernate-mapping>